<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title</title>
    <!-- Bootstrap Core CSS -->
    <link href="~/Areas/Control/Public/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!-- MetisMenu CSS -->
    <link href="~/Areas/Control/Public/vendor/metisMenu/metisMenu.min.css" rel="stylesheet">
    <!--FontAwasome-->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css">
    <!-- Custom CSS -->
    <link href="~/Areas/Control/Public/dist/css/sb-admin-2.css" rel="stylesheet">
    <!-- Morris Charts CSS -->
    <link href="~/Areas/Control/Public/vendor/morrisjs/morris.css" rel="stylesheet">
    <!-- Custom Fonts -->
    <link href="~/Areas/Control/Public/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div id="wrapper">
        <!-- Navigation -->
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Action("Index","Dashboard")">Admin Control</a>
            </div>
            <!-- /.navbar-header -->
            <ul class="nav navbar-top-links navbar-right">
                <!-- /.dropdown -->
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-user fa-fw"></i> <i class="fa fa-caret-down"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-user">
                        <li>
                            <a href="@Url.Action("Logout","Account")"><i class="fa fa-sign-out fa-fw"></i> Logout</a>
                        </li>
                    </ul>
                    <!-- /.dropdown-user -->
                </li>
                <!-- /.dropdown -->
            </ul>
            <!-- /.navbar-top-links -->
            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">

                        <li>
                            <a href="@Url.Action("Index","Dashboard")"><i class="fa fa-dashboard fa-fw"></i> Dashboard</a>
                        </li>
                        <li>
                            <a href="@Url.Action("index","Homes")"><i class="fa fa-edit fa-fw"></i> Ana səhifə başlıq</a>
                        </li>
                        <li>
                            <a href="@Url.Action("index","AboutHeaders")"><i class="fa fa-edit fa-fw"></i> Haqqımızda başlıq</a>
                        </li>
                        <li>
                            <a href="@Url.Action("index","FaqHeaders")"><i class="fa fa-edit fa-fw"></i> Suallar başlıq</a>
                        </li>
                        <li>
                            <a href="@Url.Action("index","LisenceHeaders")"><i class="fa fa-edit fa-fw"></i> Lisenziya başlıq</a>
                        </li>
                        <li>
                            <a href="@Url.Action("index","Aboutus")"><i class="fa fa-edit fa-fw"></i> Haqqımızda</a>
                        </li>

                        <li>
                            <a href="@Url.Action("index","FaqNews")"><i class="fa fa-edit fa-fw"></i> Suallar(Pexelsdə yeniyəm)</a>
                        </li>
                        <li>
                            <a href="@Url.Action("index","FaqUploads")"><i class="fa fa-edit fa-fw"></i> Suallar(Yükləmə)</a>
                        </li>
                        <li>
                            <a href="@Url.Action("index","Categories")"><i class="fa fa-edit fa-fw"></i> Kateqoriya</a>
                        </li>
                        <li>
                            <a href="@Url.Action("index","LisenceAllows")"><i class="fa fa-edit fa-fw"></i> Lisenziya(izazə verilən)</a>
                        </li>
                        <li>
                            <a href="@Url.Action("index","LisenceNotAllows")"><i class="fa fa-edit fa-fw"></i> Lisenziya(icazə verilməyən)</a>
                        </li>
                        <li>
                            <a href="@Url.Action("index","LisenceCards")"><i class="fa fa-edit fa-fw"></i> Lisenziya Kartlar</a>
                        </li>
                        <li>
                            <a href="@Url.Action("index","Photos")"><i class="fa fa-edit fa-fw"></i> Şəkillər</a>
                        </li>
                        <li>
                            <a href="@Url.Action("index","Searches")"><i class="fa fa-edit fa-fw"></i> Populyar axtarışlar</a>
                        </li>
                        <li>
                            <a href="@Url.Action("index","Settings")"><i class="fa fa-edit fa-fw"></i> Qısa məlumatlar</a>
                        </li>
                    </ul>
                </div>
                <!-- /.sidebar-collapse -->
            </div>
            <!-- /.navbar-static-side -->
        </nav>
    <div id="page-wrapper">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">@ViewBag.Title</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        @RenderBody()
                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
    </div>

        <!-- /#page-wrapper -->
    </div>
    <!-- /#wrapper -->
    <!-- jQuery -->
    <script src="~/Areas/Control/Public/vendor/jquery/jquery.min.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="~/Areas/Control/Public/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!-- Metis Menu Plugin JavaScript -->
    <script src="~/Areas/Control/Public/vendor/metisMenu/metisMenu.min.js"></script>
    <!-- Morris Charts JavaScript -->
    <script src="~/Areas/Control/Public/vendor/raphael/raphael.min.js"></script>
    <script src="~/Areas/Control/Public/vendor/morrisjs/morris.min.js"></script>
    <script src="~/Areas/Control/Public/data/morris-data.js"></script>
    <!-- Custom Theme JavaScript -->
    <script src="~/Areas/Control/Public/dist/js/sb-admin-2.js"></script>
</body>
</html>
